<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="it" original="MICROSOFT.WEB.LIBRARYMANAGER.VSIX/RESOURCES/TEXT.RESX" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6810.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="MICROSOFT.WEB.LIBRARYMANAGER.VSIX/RESOURCES/TEXT.RESX" datatype="resx">
        <trans-unit id="CleanLibrariesStarted" translate="yes" xml:space="preserve">
          <source>Clean libraries started...</source>
          <target state="translated">Pulizia delle librerie avviata...</target>
          <note from="MultilingualBuild" annotates="source" priority="2">When the "Clean" command is being executed</note>
        </trans-unit>
        <trans-unit id="CleanLibrariesSucceeded" translate="yes" xml:space="preserve">
          <source>Clean libraries succeeded</source>
          <target state="translated">La pulizia delle librerie è riuscita</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Status message</note>
        </trans-unit>
        <trans-unit id="LibraryRestoredNoChange" translate="yes" xml:space="preserve">
          <source>Restore completed. Files already up-to-date</source>
          <target state="translated">Ripristino completato. I file sono già aggiornati</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Status message</note>
        </trans-unit>
        <trans-unit id="Loading" translate="yes" xml:space="preserve">
          <source>Loading...</source>
          <target state="translated">Caricamento...</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in JSON editor for Intellisense</note>
        </trans-unit>
        <trans-unit id="PackagesCouldNotBeLoaded" translate="yes" xml:space="preserve">
          <source>Packages could not be loaded</source>
          <target state="translated">Non è stato possibile caricare i pacchetti</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in installer dialog</note>
        </trans-unit>
        <trans-unit id="CheckForUpdates" translate="yes" xml:space="preserve">
          <source>Check for updates</source>
          <target state="translated">Controlla la disponibilità di aggiornamenti</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in JSON editor light bulb</note>
        </trans-unit>
        <trans-unit id="NoUpdatesFound" translate="yes" xml:space="preserve">
          <source>No updates found at this time</source>
          <target state="translated">Al momento non sono stati trovati aggiornamenti</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in message box when light bulb found no updates</note>
        </trans-unit>
        <trans-unit id="UninstallLibrary" translate="yes" xml:space="preserve">
          <source>Uninstall {0}</source>
          <target state="translated">Disinstalla {0}</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in JSON editor light bulb</note>
        </trans-unit>
        <trans-unit id="NugetInstallPrompt" translate="yes" xml:space="preserve">
          <source>A NuGet package will be installed to augment the MSBuild process, but no files will be added to the project.

This may require an internet connection.

Do you want to continue?</source>
          <target state="translated">Verrà installato un pacchetto NuGet per potenziare il processo di MSBuild, ma non verrà aggiunto alcun file al progetto.

Per questa operazione potrebbe essere necessaria una connessione Internet.

Continuare?</target>
        </trans-unit>
        <trans-unit id="TypeToSearch" translate="yes" xml:space="preserve">
          <source><it id="1" pos="open">&lt;Type to search&gt;</it></source>
          <target state="translated">&lt;Digitare per eseguire la ricerca&gt;</target>
        </trans-unit>
        <trans-unit id="AddClientSideLibrary" translate="yes" xml:space="preserve">
          <source>Add Client-Side Library</source>
          <target state="translated">Aggiungi libreria lato client</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Add Client-Side Library</note>
        </trans-unit>
        <trans-unit id="SelectLibraryToSelectFilesToInstall" translate="yes" xml:space="preserve">
          <source><it id="1" pos="open">&lt;Choose a library to select files to install&gt;</it></source>
          <target state="translated">&lt;Scegliere una libreria per selezionare i file da installare&gt;</target>
        </trans-unit>
        <trans-unit id="DisableRestoreOnBuild" translate="yes" xml:space="preserve">
          <source>Disabl&amp;e Client-Side Libraries on Build</source>
          <target state="translated">Disa&amp;bilita ripristino delle librerie lato client durante la compilazione</target>
        </trans-unit>
        <trans-unit id="EnableRestoreOnBuild" translate="yes" xml:space="preserve">
          <source>&amp;Enable Restore Client-Side Libraries on Build...</source>
          <target state="translated">&amp;Abilita ripristino delle librerie lato client durante la compilazione...</target>
        </trans-unit>
        <trans-unit id="Nuget_InstallingPackage" translate="yes" xml:space="preserve">
          <source>Installing NuGet package containing MSBuild target...</source>
          <target state="translated">Installazione del pacchetto NuGet che contiene la destinazione MSBuild...</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageFailedToInstall" translate="yes" xml:space="preserve">
          <source>NuGet package failed to install</source>
          <target state="translated">Non è stato possibile installare il pacchetto NuGet</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageFailedToUninstall" translate="yes" xml:space="preserve">
          <source>NuGet package failed to uninstall</source>
          <target state="translated">Non è stato possibile disinstallare il pacchetto NuGet</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageInstalled" translate="yes" xml:space="preserve">
          <source>NuGet package installed</source>
          <target state="translated">Pacchetto NuGet installato</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageUninstalled" translate="yes" xml:space="preserve">
          <source>NuGet package uninstalled</source>
          <target state="translated">Pacchetto NuGet disinstallato</target>
        </trans-unit>
        <trans-unit id="Nuget_UninstallingPackage" translate="yes" xml:space="preserve">
          <source>Uninstalling NuGet package...</source>
          <target state="translated">Disinstallazione del pacchetto NuGet...</target>
        </trans-unit>
        <trans-unit id="FailToDeleteLibrariesFromProject" translate="yes" xml:space="preserve">
          <source>Could not delete libraries from project</source>
          <target state="translated">Non è stato possibile eliminare le librerie dal progetto</target>
        </trans-unit>
        <trans-unit id="LibraryDeletedFromProject" translate="yes" xml:space="preserve">
          <source>Library {0} was successfuly deleted from project</source>
          <target state="translated">La libreria {0} è stata eliminata dal progetto</target>
        </trans-unit>
        <trans-unit id="FailToAddLibrariesToProject" translate="yes" xml:space="preserve">
          <source>Could not add libraries to project</source>
          <target state="translated">Non è stato possibile aggiungere librerie al progetto</target>
        </trans-unit>
        <trans-unit id="LibraryAddedToProject" translate="yes" xml:space="preserve">
          <source>Library {0} was successfuly added to project</source>
          <target state="translated">La libreria {0} è stata aggiunta al progetto</target>
        </trans-unit>
        <trans-unit id="OutputWindowTitle" translate="yes" xml:space="preserve">
          <source>Library Manager</source>
          <target state="translated">Gestione librerie</target>
        </trans-unit>
        <trans-unit id="OperationInProgress" translate="yes" xml:space="preserve">
          <source>Library Manager operation already in progress.</source>
          <target state="translated">L'operazione di Gestione librerie è già in corso.</target>
        </trans-unit>
        <trans-unit id="Files" translate="yes" xml:space="preserve">
          <source>Files:</source>
          <target state="translated">File:</target>
        </trans-unit>
        <trans-unit id="Library" translate="yes" xml:space="preserve">
          <source>Library:</source>
          <target state="translated">Libreria:</target>
        </trans-unit>
        <trans-unit id="Provider" translate="yes" xml:space="preserve">
          <source>Provider: </source>
          <target state="translated">Provider: </target>
        </trans-unit>
        <trans-unit id="TargetLocation" translate="yes" xml:space="preserve">
          <source>Target Location:</source>
          <target state="translated">Percorso di destinazione:</target>
        </trans-unit>
        <trans-unit id="ChooseSpecificFiles" translate="yes" xml:space="preserve">
          <source>Choose specific files:</source>
          <target state="translated">Scegliere i file specifici:</target>
        </trans-unit>
        <trans-unit id="IncludeAllLibraryFiles" translate="yes" xml:space="preserve">
          <source>Include all library files</source>
          <target state="translated">Includi tutti i file di libreria</target>
        </trans-unit>
        <trans-unit id="NoFilesSelected" translate="yes" xml:space="preserve">
          <source>No files have been selected</source>
          <target state="translated">Non è stato selezionato alcun file</target>
        </trans-unit>
        <trans-unit id="Cancel" translate="yes" xml:space="preserve">
          <source>Cancel</source>
          <target state="translated">Annulla</target>
        </trans-unit>
        <trans-unit id="Install" translate="yes" xml:space="preserve">
          <source>Install</source>
          <target state="translated">Installa</target>
        </trans-unit>
      </group>
    </body>
  </file>
</xliff>