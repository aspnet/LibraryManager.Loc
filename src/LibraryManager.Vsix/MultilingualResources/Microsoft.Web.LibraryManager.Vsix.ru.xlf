<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" original="MICROSOFT.WEB.LIBRARYMANAGER.VSIX/RESOURCES/TEXT.RESX" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a" target-language="ru">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6810.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="MICROSOFT.WEB.LIBRARYMANAGER.VSIX/RESOURCES/TEXT.RESX" datatype="resx">
        <trans-unit id="CleanLibrariesStarted" translate="yes" xml:space="preserve">
          <source>Clean libraries started...</source><target state="translated">Очистка библиотек запущена...</target>
          <note from="MultilingualBuild" annotates="source" priority="2">When the "Clean" command is being executed</note>
        </trans-unit>
        <trans-unit id="CleanLibrariesSucceeded" translate="yes" xml:space="preserve">
          <source>Clean libraries succeeded</source><target state="translated">Очистка библиотек успешно завершена</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Status message</note>
        </trans-unit>
        <trans-unit id="LibraryRestoredNoChange" translate="yes" xml:space="preserve">
          <source>Restore completed. Files already up-to-date</source><target state="translated">Восстановление завершено. Файлы уже обновлены.</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Status message</note>
        </trans-unit>
        <trans-unit id="Loading" translate="yes" xml:space="preserve">
          <source>Loading...</source><target state="translated">Загрузка...</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in JSON editor for Intellisense</note>
        </trans-unit>
        <trans-unit id="PackagesCouldNotBeLoaded" translate="yes" xml:space="preserve">
          <source>Packages could not be loaded</source><target state="translated">Не удалось загрузить пакеты</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in installer dialog</note>
        </trans-unit>
        <trans-unit id="CheckForUpdates" translate="yes" xml:space="preserve">
          <source>Check for updates</source><target state="translated">Проверить наличие обновлений</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in JSON editor light bulb</note>
        </trans-unit>
        <trans-unit id="NoUpdatesFound" translate="yes" xml:space="preserve">
          <source>No updates found at this time</source><target state="translated">Обновления не найдены</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in message box when light bulb found no updates</note>
        </trans-unit>
        <trans-unit id="UninstallLibrary" translate="yes" xml:space="preserve">
          <source>Uninstall {0}</source><target state="translated">Удалить {0}</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Used in JSON editor light bulb</note>
        </trans-unit>
        <trans-unit id="NugetInstallPrompt" translate="yes" xml:space="preserve">
          <source>A NuGet package will be installed to augment the MSBuild process, but no files will be added to the project.

This may require an internet connection.

Do you want to continue?</source><target state="translated">Будет установлен пакет NuGet, который дополнит процесс сборки MSBuild, но файлы в проект добавлены не будут. Для этого может потребоваться подключение к Интернету. Вы действительно хотите продолжить?</target>
        </trans-unit>
        <trans-unit id="TypeToSearch" translate="yes" xml:space="preserve">
          <source><it id="1" pos="open">&lt;Type to search&gt;</it></source><target state="translated">&lt;Введите текст для поиска&gt;</target>
        </trans-unit>
        <trans-unit id="AddClientSideLibrary" translate="yes" xml:space="preserve">
          <source>Add Client-Side Library</source><target state="translated">Добавьте клиентские библиотеки</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Add Client-Side Library</note>
        </trans-unit>
        <trans-unit id="SelectLibraryToSelectFilesToInstall" translate="yes" xml:space="preserve">
          <source><it id="1" pos="open">&lt;Choose a library to select files to install&gt;</it></source><target state="translated">&lt;Выберите библиотеку, чтобы выбрать файлы для установки&gt;</target>
        </trans-unit>
        <trans-unit id="DisableRestoreOnBuild" translate="yes" xml:space="preserve">
          <source>Disable Restore on Build</source><target state="translated">Отключить восстановление при сборке</target>
        </trans-unit>
        <trans-unit id="EnableRestoreOnBuild" translate="yes" xml:space="preserve">
          <source>Enable Restore Client-Side Libraries on Build</source><target state="translated">Включить восстановление клиентских библиотек при сборке</target>
        </trans-unit>
        <trans-unit id="Nuget_InstallingPackage" translate="yes" xml:space="preserve">
          <source>Installing NuGet package containing MSBuild target...</source><target state="translated">Установка пакета NuGet, содержащего целевой объект MSBuild...</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageFailedToInstall" translate="yes" xml:space="preserve">
          <source>NuGet package failed to install</source><target state="translated">Не удалось установить пакет NuGet</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageFailedToUninstall" translate="yes" xml:space="preserve">
          <source>NuGet package failed to uninstall</source><target state="translated">Не удалось удалить пакет NuGet</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageInstalled" translate="yes" xml:space="preserve">
          <source>NuGet package installed</source><target state="translated">Пакет NuGet установлен</target>
        </trans-unit>
        <trans-unit id="Nuget_PackageUninstalled" translate="yes" xml:space="preserve">
          <source>NuGet package uninstalled</source><target state="translated">Пакет NuGet удален</target>
        </trans-unit>
        <trans-unit id="Nuget_UninstallingPackage" translate="yes" xml:space="preserve">
          <source>Uninstalling NuGet package...</source><target state="translated">Удаление пакета NuGet...</target>
        </trans-unit>
        <trans-unit id="FailToDeleteLibrariesFromProject" translate="yes" xml:space="preserve">
          <source>Could not delete libraries from project</source><target state="translated">Не удалось удалить библиотеки из проекта</target>
        </trans-unit>
        <trans-unit id="LibraryDeletedFromProject" translate="yes" xml:space="preserve">
          <source>Library {0} was successfuly deleted from project</source><target state="translated">Библиотека {0} успешно удалена из проекта</target>
        </trans-unit>
        <trans-unit id="FailToAddLibrariesToProject" translate="yes" xml:space="preserve">
          <source>Could not add libraries to project</source><target state="translated">Не удалось добавить библиотеки в проект</target>
        </trans-unit>
        <trans-unit id="LibraryAddedToProject" translate="yes" xml:space="preserve">
          <source>Library {0} was successfuly added to project</source><target state="translated">Библиотека {0} успешно добавлена в проект</target>
        </trans-unit>
        <trans-unit id="OutputWindowTitle" translate="yes" xml:space="preserve">
          <source>Library Manager</source><target state="translated">Диспетчер библиотек</target>
        </trans-unit>
        <trans-unit id="OperationInProgress" translate="yes" xml:space="preserve">
          <source>Library Manager operation already in progress.</source><target state="translated">Операция диспетчера библиотек уже выполняется.</target>
        </trans-unit>
        <trans-unit id="Files" translate="yes" xml:space="preserve">
          <source>Files:</source><target state="translated">Файлы:</target>
        </trans-unit>
        <trans-unit id="Library" translate="yes" xml:space="preserve">
          <source>Library:</source><target state="translated">Библиотека:</target>
        </trans-unit>
        <trans-unit id="Provider" translate="yes" xml:space="preserve">
          <source>Provider: </source><target state="translated">Поставщик: </target>
        </trans-unit>
        <trans-unit id="TargetLocation" translate="yes" xml:space="preserve">
          <source>Target Location:</source><target state="translated">Целевое расположение:</target>
        </trans-unit>
        <trans-unit id="ChooseSpecificFiles" translate="yes" xml:space="preserve">
          <source>Choose specific files:</source><target state="translated">Выберите конкретные файлы:</target>
        </trans-unit>
        <trans-unit id="IncludeAllLibraryFiles" translate="yes" xml:space="preserve">
          <source>Include all library files</source><target state="translated">Включить все файлы библиотек</target>
        </trans-unit>
        <trans-unit id="NoFilesSelected" translate="yes" xml:space="preserve">
          <source>No files have been selected</source><target state="translated">Файлы не выбраны</target>
        </trans-unit>
      </group>
    </body>
  </file>
</xliff>